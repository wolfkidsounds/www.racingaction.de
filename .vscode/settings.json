{
    "files.autoSave": "onWindowChange",
    "workbench.colorTheme": "Theme",
    "workbench.preferredDarkColorTheme": "Theme",
    "workbench.preferredHighContrastColorTheme": "Theme Mix",
    "workbench.iconTheme": "vscode-great-icons",
    "php.suggest.basic": true,
    "php-cs-fixer.formatHtml": true,
    "php-cs-fixer.onsave": true,
    "php-cs-fixer.config": ".php-cs-fixer.dist.php",
    "better-comments.highlightPlainText": true,
    "symfony-vscode.phpExecutablePath": "C:/php/php.exe",
    "symfony-vscode.routesFilters": {
        "ux_live_component": "id",
        "ux_entity_autocomplete": "id",
        "_preview_error": "id",
        "^_profiler": "id",
        "_wdt": "id",
        "fc_load_events": "id",
    },
    "php.files.exclude": [
        "**/var/**"
    ],
    "intelephense.environment.documentRoot": "./public/",
    "intelephense.files.exclude": [
        "**/.git/**",
        "**/.svn/**",
        "**/.hg/**",
        "**/CVS/**",
        "**/.DS_Store/**",
        "**/node_modules/**",
        "**/bower_components/**",
        "**/vendor/**/{Tests,tests}/**",
        "**/.history/**",
        "**/vendor/**/vendor/**",
        "**/var/**"
    ],
}